                android:layout_height="wrap_content"
                android:layout_margin="60dp"
                android:text="next "
                tools:layout_editor_absoluteX="246dp"
                tools:layout_editor_absoluteY="629dp" />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>